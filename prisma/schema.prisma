generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "windows"]
}


datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Cheese {
  id            String   @id @default(uuid()) @map("_id")
  batch         String
  quantity      Int
  state         String
  startDate     DateTime
  endDate       DateTime
  userId        String   
}